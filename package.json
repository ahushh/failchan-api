{
  "name": "express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon",
    "start:dev": "NODE_ENV=development ts-node src/index.ts",
    "start:test": "NODE_ENV=test ts-node src/index.ts",
    "start:prod": "NODE_ENV=production ts-node src/index.ts",
    "lint": "tslint --project . -c tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rm -fr dist",
    "build": "npm run clean && tsc",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "test:one": "NODE_ENV=test mocha --opts test/mocha.opts",
    "test": "NODE_ENV=test mocha --opts test/mocha.opts 'test/**/*.ts'",
    "test:coverage": "NODE_ENV=test nyc mocha --opts test/mocha.opts 'test/**/*.ts'"
  },
  "dependencies": {
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/node": "^10.11.5",
    "aws-sdk": "^2.336.0",
    "chai": "^4.2.0",
    "class-transformer": "^0.1.10",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dist-exiftool": "^10.53.0",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "glob": "^7.1.3",
    "ioredis": "^4.2.0",
    "lodash": "^4.17.11",
    "md5": "^2.2.1",
    "mocha": "^5.2.0",
    "morgan": "~1.9.0",
    "multer": "^1.4.1",
    "node-exiftool": "^2.3.0",
    "nodemon": "^1.18.4",
    "pg": "^7.5.0",
    "ramda": "^0.25.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "sharp": "^0.21.0",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.7",
    "typeorm-typedi-extensions": "^0.2.1",
    "typescript": "^3.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/ioredis": "^4.0.3",
    "@types/mocha": "^5.2.5",
    "@types/multer": "^1.3.7",
    "@types/ramda": "^0.25.39",
    "@types/redis": "^2.8.7",
    "@types/rimraf": "^2.0.2",
    "@types/sharp": "^0.21.0",
    "@types/supertest": "^2.0.6",
    "@types/uuid": "^3.4.4",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "tslint-config-airbnb": "^5.11.0"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
